{
    "manifest_version": 3,
    "name": "Timer",
    "version": "1.0",
    "permissions": ["storage", "activeTab", "tabs", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  